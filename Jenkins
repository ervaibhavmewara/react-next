pipeline {
    agent any
    stages{
        stage("build") {
            steps {
                 sh " yarn install"
                 sh " yarn build" 
            }
        }    
        stage("deploy") {
            steps {
                   
                   sh  "pm2 restart react-next || PORT=8080 pm2 start --name react-next --yarn start"

            } 
       }    
    }

}
